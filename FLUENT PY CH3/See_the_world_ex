
travel_locations = ["brazil","punta Cana", "mtl", "jamaica", "iceland"]

print(travel_locations)
#Methods are .() functions are (data inside)
print(sorted(travel_locations))
#print in reverse order without changing
print(sorted(travel_locations, reverse= True))
#print to show the list has not changed order
print(travel_locations)

print("\n")
print(travel_locations)
travel_locations.sort()
print(travel_locations)
print("\n")
travel_locations.sort(reverse=True)
print(travel_locations)

